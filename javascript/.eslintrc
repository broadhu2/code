{
  "env": {
    "es6": true
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
  "globals": {
    "console": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-console": "off",
    "no-return-await": "error",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
}